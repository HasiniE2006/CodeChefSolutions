def greet(name):
    return f"Hello, {name}!"

def capitalize(text):
    return text.upper()

# Solution as follows
def greet_and_capitalize(name):
    greeting = greet(name)
    return capitalize(greeting)

# Call the functions
name = "Alice"
final_result = greet_and_capitalize(name)

# Display the results
print("Final Result:", final_result)
